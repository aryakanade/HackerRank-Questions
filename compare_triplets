def compareTriplets(a, b):
    # Write your code here
    count_a = 0
    count_b = 0
    for i in range(len(a)):
        for j in range(len(b)):
            if i == j and a[i] > b[j]:
                count_a += 1
            elif i == j and a[i] < b[j]:
                count_b += 1
    return count_a, count_b
